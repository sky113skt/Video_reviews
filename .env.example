
# Kimi API配置（必需，与OpenAI兼容）
# 请替换为您的实际API密钥
KIMI_API_KEY=your_kimi_api_key_here
KIMI_BASE_URL=https://api.moonshot.cn/v1

# TMDB API配置（必需，用于获取电影信息）
# 请替换为您的实际API密钥
TMDB_API_KEY=your_tmdb_api_key_here

# 服务配置
